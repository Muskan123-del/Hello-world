import java.util.Scanner;
public class days;{

     public static void main(String []args){
      int mins,years,days;
      Scanner s = newScanner(System.in);
      mins = s.nextInt();
      years = mins / 365;
      mins = mins % 365,
      days = (mins / 7) % 7;  
      System.out.println("No. of years="+years);
      System.out.println("No. of days="+days);
     } 
}
